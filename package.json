{
  "name": "my-electron-vite-template",
  "version": "1.0.0",
  "description": "",
  "main": "./out/main/app.js",
  "keywords": [
    "electron",
    "vite",
    "vue3",
    "typescript",
    "education"
  ],
  "author": "xuke",
  "scripts": {
    "bootstrap": "pnpm install",
    "reinstall": "pnpm clear && pnpm bootstrap",
    "app:dev": "cross-env ENV_MODE=development electron-vite dev --watch",
    "build:dev:web": "pnpm typecheck && cross-env ENV_MODE=development electron-vite build",
    "build:dev:mac:x64": "pnpm build:dev:web && pnpm build:tsup && pnpm clear:app-resource && cross-env ENV_MODE=development electron-builder --mac --x64 --config=./dist-tsup/electron-builder.js && pnpm clear:tsup",
    "build:dev:mac:arm64": "pnpm build:dev:web && pnpm build:tsup && pnpm clear:app-resource && cross-env ENV_MODE=development electron-builder --mac --arm64 --config=./dist-tsup/electron-builder.js && pnpm clear:tsup",
    "build:dev:win:ia32": "pnpm build:dev:web && pnpm build:tsup && pnpm clear:app-resource && cross-env ENV_MODE=development electron-builder --win --ia32 --config=./dist-tsup/electron-builder.js && pnpm clear:tsup",
    "build:dev:win:x64": "pnpm build:dev:web && pnpm build:tsup && pnpm clear:app-resource && cross-env ENV_MODE=development electron-builder --win --x64 --config=./dist-tsup/electron-builder.js && pnpm clear:tsup",
    "typecheck": "vue-tsc -p tsconfig.json --composite false --noEmit",
    "build:tsup": "tsup",
    "clear:tsup": "rimraf dist-tsup",
    "clear:app-resource": "rimraf dist-electron",
    "clear": "rimraf dist-electron dist-tsup out pnpm-lock.json pnpm-lock.yaml package-lock.json yarn.lock node_modules",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install",
    "format": "pnpm run lint:eslint & pnpm run lint:prettier",
    "lint:eslint": "eslint . --cache --max-warnings 0 --fix",
    "lint:prettier": "prettier --write  \"{electron,renderer,typings}/**/*.{js,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "rebuild": "electron-rebuild -f -w better-sqlite3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@iconify/tools": "^3.0.2",
    "@swc/core": "^1.3.75",
    "@types/better-sqlite3": "^7.6.4",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.4.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/runtime-core": "^3.3.4",
    "@vueuse/core": "^10.2.1",
    "axios": "0.27.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "electron": "22.2.1",
    "electron-builder": "^24.4.0",
    "electron-log": "^4.4.8",
    "electron-rebuild": "^3.2.9",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.1",
    "electron-vite": "^1.0.27",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.4",
    "postcss": "^8.4.25",
    "prettier": "2.8.4",
    "qs": "^6.11.2",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.2",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "unplugin-icons": "^0.16.3",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.1",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^9.3.1",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.4"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@electron/remote": "^2.0.10",
    "better-sqlite3": "^8.5.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  }
}